# Databricks Configuration
# Copy this file to .env and fill in your actual values

# Your Databricks workspace URL (without https://)
DATABRICKS_HOST=your-workspace.cloud.databricks.com

# Your personal access token from Databricks
DATABRICKS_TOKEN=your-personal-access-token

# Your cluster ID (optional, for databricks-connect)
DATABRICKS_CLUSTER_ID=your-cluster-id

# Your organization ID (optional)
DATABRICKS_ORG_ID=your-org-id

# Port for databricks-connect (default: 15001)
DATABRICKS_PORT=15001

# Database and Schema
CATALOG_NAME=hive_metastore
SCHEMA_NAME=default

# Azure Storage (if using)
STORAGE_ACCOUNT=your-storage-account
CONTAINER_NAME=your-container
